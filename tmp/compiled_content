/*
 * #001: Reset
 *
 * Mostly by Eric Meyer. See:
 *
 * - http://meyerweb.com/eric/thoughts/2007/05/01/reset-reloaded/
 * - http://html5doctor.com/html-5-reset-stylesheet/
 * - http://www.aestheticallyloyal.com/public/optimize-legibility/
 * - http://maxvoltar.com/archive/-webkit-font-smoothing
 * - http://people.opera.com/patrickl/experiments/keyboard/test
 * - http://www.pathf.com/blogs/2008/05/formatting-quoted-code-in-blog-posts-css21-white-space-pre-wrap/
 * - http://code.flickr.com/blog/2008/11/12/on-ui-quality-the-little-things-client-side-image-resizing/
 *
 * The reset styles do not just counter everything in the default browser
 * stylesheet, but may also be used to define basic element styling across
 * media.
 */
html, body, div, span, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote,
pre, abbr, address, cite, code, del, dfn, em, img, ins, kbd, q, samp, small,
strong, sub, sup, var, b, i, dl, dt, dd, ol, ul, li, fieldset, form, label,
legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside,
canvas, details, figcaption, figure, footer, header, hgroup, menu, nav,
section, summary, time, mark, audio, video {
  margin: 0;
  padding: 0;
  border: 0;
  font-size: 100%;
  font: inherit;
  vertical-align: baseline; }

article, aside, details, figcaption, figure, footer, header, hgroup, menu,
nav, section {
  display: block; }

p, table, fieldset, ol, ul, dl, pre, h1, h2, h3, h4, h5, h6, blockquote {
  margin: 1.6 0; }

body {
  color: black;
  background: white; }

ol, ul {
  list-style: none; }

table {
  font-size: inherit;
  font: 100%;
  border-collapse: separate;
  border-spacing: 0; }

caption, th, td {
  text-align: left;
  font-weight: normal; }

blockquote:before, blockquote:after, q:before, q:after {
  content: "";
  content: none; }

blockquote, q {
  quotes: none; }

strong {
  font-weight: bold; }

em {
  font-style: italic; }

ins {
  background-color: #ff9;
  color: #000;
  text-decoration: none; }

mark {
  background-color: #ff9;
  color: #000;
  font-style: italic;
  font-weight: bold; }

del {
  text-decoration: line-through; }

abbr[title], dfn[title] {
  border-bottom: 1px dotted #000;
  cursor: help; }

hr {
  display: block;
  height: 1px;
  border: 0;
  border-top: 1px solid #ccc;
  margin: 1.6 0;
  padding: 0; }

select, input, textarea, button {
  font: 99% sans-serif; }

input, select {
  vertical-align: middle; }

body, select, input, textarea {
  color: #444;
  line-height: 1.6; }

h1, h2, h3, h4, h5, h6 {
  text-rendering: optimizeLegibility; }

html {
  overflow-y: scroll; }

sub, sup {
  font-size: smaller;
  line-height: 0; }

sub {
  vertical-align: sub; }

sup {
  vertical-align: super; }

pre {
  padding: 15px;
  white-space: pre;
  /* CSS2 */
  white-space: pre-wrap;
  /* CSS 2.1 */
  white-space: pre-line;
  /* CSS 3 (and 2.1 as well, actually) */
  word-wrap: break-word;
  /* IE */ }

pre, code, kbd, samp {
  font-family: monospace, sans-serif; }

input[type="radio"] {
  vertical-align: text-bottom; }

input[type="checkbox"] {
  vertical-align: bottom; }

.ie7 input[type="checkbox"] {
  vertical-align: baseline; }

.ie6 input {
  vertical-align: text-bottom; }

label, input[type="button"], input[type="submit"], button {
  cursor: pointer; }

textarea {
  overflow: auto; }

.ie6 legend, .ie7 legend {
  margin-left: -7px; }

button {
  width: auto;
  overflow: visible; }

button, input, select, textarea {
  margin: 0; }

.ie7 img {
  -ms-interpolation-mode: bicubic; }

